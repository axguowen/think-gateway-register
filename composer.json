{
    "name": "axguowen/think-gateway-register",
    "description": "Gateway Resgister Service For ThinkPHP",
    "keywords": [
        "gateway",
        "register",
        "thinkphp"
    ],      
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "axguowen",
            "email": "axguowen@qq.com"
        }
    ],
    "require": {
        "workerman/gateway-worker": ">=3.0.0",
        "topthink/framework": "^6.1 || ^8.0",
        "ext-fileinfo": "*"
    },
    "autoload": {
        "psr-4": {
            "think\\gateway\\register\\": "src"
        }
    },
    "extra": {
        "think":{
            "services":[
                "think\\gateway\\register\\Service"
            ],
            "config": {
                "gatewayregister": "src/config.php"
            }
        }
    }
}
